{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++-12 build active file",
            "command": "/usr/bin/g++-12",
            "args": [
                "-std=gnu++2b",
                "-g",
                "-Og",
                "-DLOCAL",
                "-D_GLIBCXX_DEBUG",
                "-I/opt/ac-library",
                "-I/opt/boost/gcc/include",
                "-L/opt/boost/gcc/lib",
                "-I${workspaceFolder}/library/dump",
                "-o",
                "${fileDirname}/a.out",
                "${file}",
                "-lgmpxx",
                "-lgmp",
                "-I/usr/include/eigen3",
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}
